{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@gram-data/gram/(.*)": "<rootDir>/../libs/gram/src/$1",
    "@gram-data/gram": "<rootDir>/../libs/gram/src"
  }
}